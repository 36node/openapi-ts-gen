/* eslint-disable */
{{#each components}}
{{#if description ~}}
/** {{description}} */
{{/if}}
{{#if isRequest}}
export interface {{name}} {
  {{#each pathFields}}
  {{#if description}}
  /** {{description}} */
  {{/if}}
  {{name}}{{#unless required}}?{{/unless}}: {{type}};
  {{/each}}
  {{#if query}}
  query: {{query}}
  {{/if}}
  {{#if body}}
  body: {{body}}
  {{/if}}
}
{{#if query}}
export namespace {{name}} {
  export interface Query {
    {{#each queryFields}}
    {{#if description}}
    /** {{description}} */
    {{/if}}
    {{name}}{{#unless required}}?{{/unless}}: {{type}};
    {{/each}}
  }
}
{{/if}}
{{else}}
export interface {{name}}{{#if parents}} {{asExtends parents}}{{/if}} {
  {{#each fields}}
  {{#if description}}
  /** {{description}} */
  {{/if}}
  {{name}}{{#unless required}}?{{/unless}}: {{type}};
  {{/each}}
}
{{#if enumTypes ~}}
export namespace {{name}} {
  {{#each subTypes}}
  export interface {{name}} {
    {{#each fields}}
    {{name}}: {{type}}",
    {{/each}}
  }
  {{/each}}
  {{#each enumTypes}}
  export enum {{name}} {
    {{#each items}}
    {{this}} = "{{this}}",
    {{/each}}
  }
  {{/each}}
}
{{/if}}
{{/if}}
{{/each}}
